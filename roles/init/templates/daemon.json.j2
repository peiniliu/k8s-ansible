{
   "exec-opts": ["native.cgroupdriver={{ cgroupdriver }}"],
   {% if mirror_accelerator is not none %}
   "registry-mirrors": ["{{ mirror_accelerator }}"],
   {% endif %}
   "log-driver": "json-file",
   "log-opts": {
       "max-size": "100m",
       "max-file": "10"
   },
   "oom-score-adjust": -1000,
   "data-root": "{{ docker_storage_path }}" {%,%}
   "insecure-registries":["172.30.0.49:5000"] %}
}
